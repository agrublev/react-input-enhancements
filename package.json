{
  "name": "fkit-input-enhancements",
  "version": "1.0.1",
  "description": "Set of enhancements for input control (bootstrap-ready)",
  "scripts": {
    "test": "yarn test-jsdom && yarn test-node",
    "build": "NODE_ENV=production webpack",
    "build2": "rm -rf lib && NODE_ENV=production babel src --out-dir lib",
    "build-demo": "parcel build demo/src/index.html --no-cache",
    "build-demo2": "rm -rf demo/dist && NODE_ENV=production webpack -p",
    "stats": "webpack --profile --json > stats.json",
    "start-demo": "parcel demo/src/index.html --no-cache",
    "start": "webpack-dev-server",
    "lint": "eslint --max-warnings 0 src/*.js* src/**/*.js* demo/src/**/*.js* webpack.config.js",
    "format": "prettier-eslint --write --single-quote src/*.js* src/**/*.js* demo/src/**/*.js* webpack.config.js",
    "preversion": "yarn lint && yarn test",
    "version": "yarn build && yarn build-demo && git add -A .",
    "postversion": "git push && git push --tags",
    "gh": "git subtree push --prefix demo/dist origin gh-pages"
  },
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/agrublev/react-input-enhancements"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-loader": "^8.0.6",
    "babel-runtime": "^6.26.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "raf": "^3.4.1",
    "raw-loader": "^3.1.0",
    "react-bootstrap": "^0.32.4",
    "react-hot-loader": "^4.12.15",
    "react-test-renderer": "^16.11.0",
    "react-transform-hmr": "^1.0.4",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "latest",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "webpack-simple-progress-plugin": "^0.0.4"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "dependencies": {
    "inline-style-prefixer": "^2.0.1",
    "lodash.sortby": "^4.7.0",
    "moment-mini": "^2.22.1",
    "prop-types": "^15.6.0",
    "react": "^16.11.0",
    "react-base16-styling": "^0.6.0",
    "react-dom": "^16.11.0"
  },
  "author": "Alexander <alexkuz@gmail.com> (http://kuzya.org/)",
  "license": "MIT"
}
